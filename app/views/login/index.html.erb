<%= link_to("<< To Home", :controller => 'home', :action => 'new') %>

<div class="login index">
  <h2>Sign In</h2>

  <%= form_tag({:controller => 'dashboard',:action => 'index'}, method: 'get') do |f| %>

      <tr>
        <th>Username</th>
        <!-- nil is only a placeholder to make it work, will remove later when we get backend-->
        <td><%= text_field_tag(nil) %></td> 
      </tr>
      <tr>
        <th>Password</th>
        <td><%= text_field_tag(nil) %></td>
      </tr>
    <div class="form-buttons">
      <%= submit_tag("Sign in") %>
    </div>

  <% end %>
</div>

